doctype html
html(lang='en')
    head
        // Required meta tags
        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
        // Bootstrap CSS
        link(rel='stylesheet', href='https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css', integrity='sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z', crossorigin='anonymous')
        title Time Log
    body
        
        .container
            .jumbotron
                .card
                    .card-header
                        h1 Time Log
                    .card-body
                        h5.card-title Special title treatment
                        table.table.table-dark
                            thead
                                tr
                                    th(scope='col') #
                                    th(scope='col') Name
                                    th(scope='col') Time In
                                    th(scope='col') Time Out
                                    th(scope='col') Edit
                                    th(scope='col') Delete
                            tbody
                                tr
                                    th(scope='row') 1
                                    td Mark Otto
                                    td 5:25 AM
                                    td 5:00 PM
                                    td
                                        button.btn.btn-primary.btn-sm(type='submit') Edit
                                    td 
                                        button.btn.btn-danger.btn-sm(type='submit') Delete
                                tr
                                    th(scope='row') 2
                                    td Jacob Thornton
                                    td 5:25 AM
                                    td 5:00 PM
                                    td
                                        button.btn.btn-primary.btn-sm(type='submit') Edit
                                    td 
                                        button.btn.btn-danger.btn-sm(type='submit') Delete
                                tr
                                    th(scope='row') 3
                                    td Larry The Bird
                                    td 5:25 AM
                                    td 5:00 PM
